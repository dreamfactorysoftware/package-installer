{
	"name":              "dreamfactory/portal-sandbox",
	"description":       "DreamFactory Services Platform(tm) Portal Sandbox Application",
	"homepage":          "http://dreamfactorysoftware.github.io/#oasys",
	"version":           "1.1.20",
	"keywords":          [
		"dreamfactory", "oasys", "oauth", "authentication", "framework", "examples", "portal"
	],
	"type":              "dreamfactory-dsp-plugin",
	"license":           [
		"Apache-2.0"
	],
	"authors":           [
		{
			"name":  "Jerry Ablan",
			"email": "jerryablan@dreamfactory.com"
		}
	],
	"support":           {
		"email":  "support@dreamfactory.com",
		"issues": "https://github.com/dreamfactorysoftware/portal-sandbox/issues",
		"wiki":   "https://github.com/dreamfactorysoftware/portal-sandbox/wiki",
		"source": "https://github.com/dreamfactorysoftware/portal-sandbox"
	},
	"minimum-stability": "dev",
	"require":           {
		"php":                            ">=5.3.18",
		"kisma/kisma":                    "*",
		"dreamfactory/package-installer": "~1.2.8"
	},
	"autoload":          {
		"files": ["autoload.php"]
	},
	"extra":             {
		"type":              1,
		"name":              "Portal Sandbox",
		"api-name":          "pbox",
		"description":       "A sample application that demonstrates the DSP's portal service.",
		"start-url":         "/index.php",
		"url-is-external":   false,
		"import-url":        "https://github.com/dreamfactorysoftware/portal-sandbox/archive/master.zip",
		"active":            true,
		"fullscreen":        false,
		"fullscreen-toggle": true,
		"toggle-location":   "top",
		"config":            "config/app.config.php",
		"namespaces":        [
			{
				"namespace\\": "/path/to/code"
			},
			{
				"namespace2\\": "/path/to/code2"
			}
		],
		"links":             [
			{
				"target": null,
				"link":   "pbox"
			}
		],
		"routes":            [
			{
				"pattern": "actions"
			}
		]
	}
}
